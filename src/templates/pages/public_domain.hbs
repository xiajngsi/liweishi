---
tip-bottom: 开户注册
current: 客户公海
---
<div id="content">

  {{> content-header }}

    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <div class="widget-box">
                    <div class="widget-title"><span class="icon"><i class="icon-th"></i></span>
                        <h5>销售线索</h5>
                    </div>
                    <button type="button" class="btn btn-success pull-right" style="margin: 4px 4px;" data-toggle="modal" data-target="#addLeads">
                        <i class="icon-plus">转到销售线索</i>
                    </button>
                    <!--bootstrap table-->
                    <!--data-url="http://mikepenz.com/jsfiddle/" data-show-refresh="true"-->
                    <table data-toggle="table"
                           data-url="http://mikepenz.com/jsfiddle/"
                           data-pagination="true"
                           checkboxHeader="true"
                           data-side-pagination="server"
                           data-page-list="[5, 10, 20, 50, 100, 200]"
                           data-height="490" id="modUserTable"
                           data-toolbar="#toolbar">
                        <thead>
                        <tr>
                            <th data-field="state" data-checkbox="true"></th>
                            <th data-field="id" >录入时间</th>
                            <th data-field="name">店铺名称</th>
                            <th data-field="price" >店铺属性</th>
                            <th data-field="price" >间数</th>
                            <th data-field="price" >店铺定位</th>
                            <th data-field="price" >联系人</th>
                            <th data-field="price" >职位</th>
                            <th data-field="price" >手机号</th>
                            <th data-field="price" >省份</th>
                            <th data-field="price" >城区</th>
                            <th data-field="price" >区/县</th>
                            <th data-field="price" >跟进状态</th>
                           <!-- <th data-field="operate" data-formatter="pencil" data-events="operateEvents">修改</th>--><!--operateFormatter-->
                        </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

